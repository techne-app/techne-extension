{
  "manifest_version": 3,
  "name": "HNav",
  "version": "1.0",
  "description": "An extension that helps navigate Hacker News with AI generated tags.",
  "permissions": ["activeTab"],
  "host_permissions": [
    "https://techne.app/*"
  ],
  "icons": {
    "48": "images/icon-48.png"
  },
  "content_scripts": [
    {
      "matches": ["https://news.ycombinator.com/*"],
      "js": ["content.js"]
    }
  ],
  "default_locale": "en"
}
